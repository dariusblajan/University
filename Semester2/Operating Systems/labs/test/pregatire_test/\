#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(){
	FILE *f;
	int count = 0;
	f = fopen("prosir.in", "rt");
	char *s;
	char c;
	s = (char*)malloc(sizeof(char));
	s[0] = '\0';
	while(1){
		c = fgetc(f);
		if(c == EOF){
			break;
		} else{
			s = (char*)realloc(s, (count+2)*sizeof(char));
			s[count] = c;
			count ++;
			s[count] = '\0';
			printf("%c ", c);
		}
	}
	return 0;
}
